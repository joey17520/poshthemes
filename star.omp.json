{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "blocks": [
    {
      "alignment": "left",
      "segments": [
        {
          "foreground": "#16ffd4",
          "properties": {
            "style": "folder"
          },
          "style": "plain",
          "template": "\uf487 {{ .UserName }} ",
          "type": "session"
        },
        {
          "foreground": "#f6f345",
          "properties": {
            "style": "folder"
          },
          "style": "plain",
          "template": "<#F3EFF5>at</> \ue5fe {{ .Path }} ",
          "type": "path"
        },
        {
          "foreground": "#F141A8",
          "properties": {
            "fetch_status": true
          },
          "style": "plain",
          "template": "<#ffffff>on</> {{ .HEAD }}{{if .BranchStatus }} {{ .BranchStatus }}{{ end }}{{ if .Working.Changed }} \uf044 {{ .Working.String }}{{ end }}{{ if and (.Working.Changed) (.Staging.Changed) }} |{{ end }}{{ if .Staging.Changed }} \uf046 {{ .Staging.String }}{{ end }} ",
          "type": "git"
        },
        {
          "foreground": "#d95d3d",
          "properties": {
            "fetch_version": true
          },
          "style": "plain",
          "template": "<#F3EFF5>via</> \ue7a8 {{ .Full }} ",
          "type": "rust"
        },
        {
          "foreground": "#C678DD",
          "properties": {
            "fetch_version": true
          },
          "style": "plain",
          "template": "<#F3EFF5>via</> \ue235 {{ if .Venv }}{{ .Venv }} {{ end }}{{ .Full }}",
          "type": "python"
        },
        {
          "foreground": "#0dc4f2",
          "properties": {
            "fetch_version": true
          },
          "style": "plain",
          "template": "<#F3EFF5>via</> \ue627 {{ .Full }} ",
          "type": "go"
        },
        {
          "foreground": "#98C379",
          "properties": {
            "fetch_version": true
          },
          "style": "plain",
          "template": "<#F3EFF5>via</> \ue781 {{ if .PackageManagerIcon }}{{ .PackageManagerIcon }} {{ end }}{{ .Full }} ",
          "type": "node"
        },
        {
          "foreground": "#FE4A49",
          "style": "plain",
          "template": "<#F3EFF5>and</> \ue71e {{.Full}} ",
          "type": "npm"
        },
        {
          "foreground": "#ce092f",
          "properties": {
            "fetch_version": true
          },
          "style": "plain",
          "template": "<#F3EFF5>via</> \ue738 {{ .Full }} ",
          "type": "java"
        },
        {
          "foreground": "#06aad5",
          "properties": {
            "fetch_version": true
          },
          "style": "plain",
          "template": "<#F3EFF5>via</> \ue77f {{ .Full }} ",
          "type": "dotnet"
        }
      ],
      "type": "prompt"
    },
    {
      "alignment": "left",
      "newline": true,
      "segments": [
        {
          "foreground": "#63F08C",
          "style": "plain",
          "template": "\u279c ",
          "type": "text"
        }
      ],
      "type": "prompt"
    }
  ],
  "version": 3
}
